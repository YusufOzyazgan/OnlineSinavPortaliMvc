
@{
    ViewData["Title"] = "GetUserList";
}

@model List<OnlineSinavPortali.ViewModels.UserModel>
<h1 class="mt-3">Kullanıcılar</h1>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Adı Soyadı</th>
            <th>E-Posta</th>
            <th>Kullanıcı Adı</th>
            <th>Öğrenci Numarası</th>
            <th>Rolü</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FullName</td>
                <td>@item.Email</td>
                <td>@item.Username</td>
                <td>@item.StudentNumber</td>
                
            </tr>
        }

    </tbody>
</table>

